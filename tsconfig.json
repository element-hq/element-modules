{
    "$schema": "http://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": ["dom", "es2022", "esnext"],
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "declaration": true
    }
}
